<view class="page-container job-container"><uni-nav-bar vue-id="296091a0-1" dark="{{true}}" fixed="{{true}}" status-bar="{{true}}" title="我的报名" bind:__l="__l"></uni-nav-bar><view class="pl-3 pr-3 pb-3 pt-1"><view class="job_tab"><uni-segmented-control vue-id="296091a0-2" current="{{currentTab}}" values="{{tabs}}" data-event-opts="{{[['^clickItem',[['onClickItem']]]]}}" bind:clickItem="__e" bind:__l="__l"></uni-segmented-control></view><block wx:if="{{user_token}}"><view class="recommend-box"><view class="box-content"><block wx:for="{{tab_content_list}}" wx:for-item="cell" wx:for-index="index" wx:key="index"><view class="recommend-item p-3 mt-2"><view data-event-opts="{{[['tap',[['goDetail',['$0'],[[['tab_content_list','',index]]]]]]]}}" bindtap="__e"><view class="header d-flex justify-content-between align-items-center"><text class="d-block fs_14 font-weight-semibold">{{cell.title}}</text><text class="d-block fs_15 font-weight-semibold pay"><block wx:if="{{cell.status==1&&(cell.is_check==2||cell.is_check==3)}}"><text>已报名</text></block><block wx:else><block wx:if="{{cell.status==2&&(cell.is_check==2||cell.is_check==3)}}"><text>进行中</text></block><block wx:else><block wx:if="{{cell.status==3}}"><text>已完成</text></block></block></block></text></view><view class="content mt-2 mb-2">{{''+cell.brief+''}}</view><view class="footer"><view><uni-icons vue-id="{{'296091a0-3-'+index}}" type="calendar-filled" size="14" bind:__l="__l"></uni-icons><text class="fs_11 ml-1">{{cell.startdate+" - "+cell.enddate}}</text></view><view class="d-flex justify-content-between align-items-center mt-1"><view class="d-flex align-items-center"><uni-icons vue-id="{{'296091a0-4-'+index}}" type="location-filled" size="14" bind:__l="__l"></uni-icons><text class="fs_11 ml-1">{{cell.name}}</text></view><view class="d-flex align-items-center"><view class="d-flex align-items-center mr-3"><image class="image" style="width:24rpx;height:24rpx;" src="{{personsIcon}}" mode="scaleToFill"></image><text class="fs_11 ml-1">{{cell.numbers}}</text></view><view class="d-flex align-items-center"><uni-icons vue-id="{{'296091a0-5-'+index}}" type="eye-filled" size="14" bind:__l="__l"></uni-icons><text class="fs_11 ml-1">{{cell.all_views}}</text></view></view></view></view></view><block wx:if="{{cell.status==1&&cell.is_check==2}}"><view class="d-flex justify-content-end mt-2"><text data-event-opts="{{[['tap',[['cancelEnroll',['$0'],[[['tab_content_list','',index]]]]]]]}}" class="bg-primary-600 my-btn py-2 px-4" bindtap="__e">取消报名</text></view></block><block wx:else><block wx:if="{{cell.status==3}}"><view class="d-flex justify-content-end mt-2"><text class="bg-primary-600 my-btn py-2 px-4">{{"工资 ￥"+cell.money}}</text></view></block></block></view></block><uni-load-more vue-id="296091a0-6" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></view></view></block><block wx:else><label data-event-opts="{{[['tap',[['goAuth',['$event']]]]]}}" class="bg-primary-600 fs_16 no-auth-btn _span" bindtap="__e">请点击登录</label></block></view></view>