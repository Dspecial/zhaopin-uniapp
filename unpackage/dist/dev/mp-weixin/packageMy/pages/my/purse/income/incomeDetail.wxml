<view class="page-container purse-container"><uni-nav-bar vue-id="389f50e3-1" dark="{{true}}" fixed="{{true}}" status-bar="{{true}}" left-icon="left" border="{{false}}" title="金额明细" data-event-opts="{{[['^clickLeft',[['goBack']]]]}}" bind:clickLeft="__e" bind:__l="__l"></uni-nav-bar><view class="detail-list"><block wx:if="{{user_token}}"><block wx:for="{{lists}}" wx:for-item="item" wx:for-index="index" wx:key="index"><view class="d-flex align-content-start detailList-item pl-3 pr-3"><view class="w-20 date pt-3 pb-3"><text class="fs_25">{{item.date_y}}</text><text class="fs_12 ml-1">{{item.date_m+" 月"}}</text></view><view class="w-80"><block wx:for="{{item.list}}" wx:for-item="cell" wx:for-index="j" wx:key="j"><view class="content pt-3 pb-3"><view class="d-flex align-items-center justify-content-between"><view class="mr-3"><text class="fs_14 d-block">{{cell.title}}</text><text class="fs_12 text-grey-200 d-block">{{cell.remark}}</text></view><text class="fs_17 text-warning">{{(cell.type==1?'+':'-')+cell.money}}</text></view><text class="fs_12 text-grey-200 d-flex justify-content-end">{{cell.date_time}}</text></view></block></view></view></block><uni-load-more vue-id="389f50e3-2" status="{{loadStatus}}" bind:__l="__l"></uni-load-more></block><block wx:else><label data-event-opts="{{[['tap',[['goAuth',['$event']]]]]}}" class="bg-primary-600 fs_16 no-auth-btn _span" bindtap="__e">请点击登录</label></block></view></view>